# -*- mode: ruby -*-
# vi: set ft=ruby :
#

Vagrant.configure(2) do |config|

  config.vm.define "ubuntu14" do |ubuntu|

    ubuntu.vm.box = "maier/ubuntu-14.04.3-x86_64"

    ubuntu.vm.synced_folder ".", "/vagrant", disabled: false

    ubuntu.vm.provider "virtualbox" do |vb|
      vb.name = "ubuntu14"
      vb.cpus = 1
      vb.memory = "1024"
    end

  end

end
